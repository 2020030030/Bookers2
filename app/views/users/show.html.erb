 <main>
<%= render'users/list', user:@user,book:@book%>
   <div class='col-md-9'>
     <h2>Books</h2>
 <table class='table table-hover table-inverse'>

 <thead>
    <th>Title</th>
    <th>Opinion</th>
    <th colspan="3"></th>
  </thead>

 <% @.each do |book| %>
  <tr>
  <td><%= attachment_image_tag @user, :profile_image, fallback: "no_image.jpg",size:"40x40" %></td>
     <td><%= link_to book.title, book_path(book.id)%></td>
      <td><%= book.body %></td>
  </tr>
 <% end %>
  </table>
</div>
</main>