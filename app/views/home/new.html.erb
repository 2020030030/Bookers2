  <main>
 <%= render'books/list',user:@user,book:@book%>
  <div class='col-md-9'>
     <h2>Books</h2>
 <table class='table table-hover table-inverse'>

      <thead>
        <th>Title</th>
        <th>Opinion</th>
        <th colspan="3"></th>
     </thead>
     <% @books.each do |book| %>
     <tr>
    <%= attachment_image_tag @post_image, :image %>
     <td><%= book.title %></td>
      <td><%= book.body %></td>
     </tr>
      <% end %>

  </table>

</div>

</main>
